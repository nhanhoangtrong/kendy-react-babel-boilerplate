{
  "name": "kendy-react-babel-boilerplate",
  "version": "1.0.0",
  "description": "My React app bootstrap project for quick development",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && http-server ./dist",
    "build:dll":
      "cross-env NODE_ENV=development webpack --progress --config dll.webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "analyzer": "cross-env ANALYZING=true npm run build",
    "lint": "eslint --ext .jsx --ext .js --fix .",
    "test": "jest",
    "coverage": "jest --coverage",
    "prettier": "prettier --write src/**/*.{scss,js,jsx,json}"
  },
  "repository": {
    "type": "git",
    "url":
      "git+https://github.com/nhanhoangtrong/kendy-react-babel-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "immutable",
    "boilerplate",
    "javascript",
    "JSX",
    "webpack"
  ],
  "author": "Kendy Hoang",
  "license": "MIT",
  "bugs": {
    "url":
      "https://github.com/nhanhoangtrong/kendy-react-babel-boilerplate/issues"
  },
  "homepage":
    "https://github.com/nhanhoangtrong/kendy-react-babel-boilerplate#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fastclick": "^3.0.2",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "^2.2.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sanitize.css": "^7.0.1"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^1.0.0",
    "dotenv": "6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.4.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.1.0",
    "http-server": "^0.11.1",
    "jest": "^23.5.0",
    "nib": "^1.1.2",
    "path-is-inside": "^1.0.2",
    "postcss": "^7.0.2",
    "postcss-autoreset": "^2.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.11.1",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.12.1",
    "style-loader": "^0.23.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.3.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": ["js", "jsx"],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "@app(.*)$": "<rootDir>/src$1"
    }
  }
}
